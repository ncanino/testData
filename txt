  SAVEPOINT before_alter;

  -- Aggiunta delle colonne
  BEGIN
    ALTER TABLE BNLTLPUSR.SUBSCRIPTION
    ADD (SPORTELLO_CONTO VARCHAR2(100),
         STATO_ACCORDI VARCHAR2(50));
  EXCEPTION
    WHEN OTHERS THEN
      ROLLBACK TO before_alter;
      RAISE;
  END;

  COMMIT;
END;



